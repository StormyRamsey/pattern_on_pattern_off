<div class="heading">
<h1>New Pattern</h1>
</div>

<div class="new_form">
<%= form_for @pattern do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :designer %>
  <%= f.text_field :designer_name %>

  <%= f.label :details %>
  <%= f.text_field :details_body %>

  <%= f.submit class: "update" %>
<% end %>
</div>
